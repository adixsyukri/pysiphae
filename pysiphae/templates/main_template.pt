                                <button class="btn btn-default" type="button">
                                    <i class="fa fa-search"></i>
                                </button>
                            </span>
                            </div>
                            <!-- /input-group -->
                        </li>
                        <li tal:repeat="link view.main_navigation">
                            <a tal:attributes="href link['href']" tal:content="link['label']">
                                Link
                            </a>
                        </li>
                        <metal:sidebar define-slot="sidebar"/>
                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>

        <div id="page-wrapper">
            <div class="row">
               <div class="col-lg-12">
                    <metal:header define-slot="header"/>
                </div>
             </div>
            <div>
            <metal:content define-slot="content"/>
