    <script src="/++static++/bower_components/morrisjs/morris.min.js"></script>

    <!-- DC.js -->

    <script type="text/javascript" src="/++static++/bower_components/d3/d3.min.js"></script>
    <script src="/++static++/bower_components/d3-tip/index.js"></script>
    <script type="text/javascript" src="/++static++/bower_components/crossfilter/crossfilter.js"></script>
    <script type="text/javascript" src="/++static++/bower_components/dcjs/dc.js"></script>
    <script type="text/javascript" src="/++static++/bower_components/leaflet/dist/leaflet.js"></script>
    <script type="text/javascript" src="/++static++/bower_components/leaflet.markercluster/dist/leaflet.markercluster.js"></script>
    <script type="text/javascript" src="/++static++/bower_components/dc-addons/dist/leaflet-map/dc-leaflet.js"></script>
    <script src="/++static++/bower_components/dc-addons/dist/bubble-cloud/dc-bubble-cloud.js"></script>
    <script src="/++static++/bower_components/dc-addons/dist/tooltip/dc-tooltip-mixin.js"></script>

    <script type="text/javascript" src="/++static++/bower_components/reductio/reductio.js"></script>
    <!-- Custom Theme JavaScript -->

    <metal:slot define-slot="javascript_footer_slot"></metal:slot>

